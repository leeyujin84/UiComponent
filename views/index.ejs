<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="/javascripts/tab.js"></script>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <div style="width:100%;height: 700px;display: flex">
    <div style="float: left; width: 300px; border: 0px solid">
      <ul>
        <li><a href="javascript:void(0);" onclick="fn_addTab('page1','page1','/pagecontrol/page1')">PageControl1</a></li>
        <li><a href="javascript:void(0);" onclick="fn_addTab('page2','page2','/pagecontrol/page2')">PageControl2</a></li>
        <li><a href="javascript:void(0);" onclick="fn_addTab('page3','page3','/pagecontrol/page3')">PageControl3</a></li>
        <li><a href="javascript:void(0);" onclick="fn_addTab('page4','page4','/pagecontrol/page4')">PageControl4</a></li>
        <li><a href="javascript:void(0);" onclick="fn_addTab('page5','page5','/pagecontrol/page5')">PageControl5</a></li>
      </ul>
    </div>
    <div id="pageControl" style="width: 100%; height: 700px">
    </div>
    </div>
  </body>
  <script>

      var pageControl = new PageControl({
          id : "pageControl",
          height : "700px"
      });
      function fn_addTab(id, title, src){
          pageControl.add({
              id: id,
              src: src,
              title:title
          })
      }

  </script>
</html>
